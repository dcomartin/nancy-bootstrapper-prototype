{
  "version": "1.0.0-*",

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "warningsAsErrors": true,
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules"
      ]
    }
  },

  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Nancy.AspNetCore": { "target": "project" }
  },

  "frameworks": {
    "net451": { },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  },

  "publishOptions": {
    "exclude": [
      "**.user",
      "**.vspscc"
    ]
  }
}
